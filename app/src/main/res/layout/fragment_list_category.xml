                android:id="@+id/btn4"
                android:padding="@dimen/bottomabout"
                app:buttonColor="@color/teal_200"
                android:src="@mipmap/ic_home_foreground"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:text="All Quotes"
                android:textColor="@color/white"
                android:gravity="bottom"
                android:layout_below="@+id/btn4"
                android:layout_centerHorizontal="true"
                android:textSize="@dimen/select_top_bottom_margin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </RelativeLayout>

        <RelativeLayout
            android:padding="@dimen/bottomabout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.app.ourquotes.custombutton.SquareButton
                android:id="@+id/btn5"
                android:padding="@dimen/bottomabout"
                app:buttonColor="@color/teal_200"
                android:src="@mipmap/ic_home_foreground"
